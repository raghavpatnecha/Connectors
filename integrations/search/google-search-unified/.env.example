# Google Custom Search API Configuration
GOOGLE_PSE_API_KEY=your_google_custom_search_api_key_here
GOOGLE_PSE_ENGINE_ID=your_search_engine_cx_id_here

# OAuth Configuration (if using full OAuth integration)
GOOGLE_CLIENT_ID=your_oauth_client_id_here
GOOGLE_CLIENT_SECRET=your_oauth_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:3139/oauth/callback

# Server Configuration
HTTP_PORT=3139
NODE_ENV=development
LOG_LEVEL=info

# Vault Configuration (for production OAuth)
VAULT_ADDR=http://localhost:8200
VAULT_TOKEN=your_vault_token_here
