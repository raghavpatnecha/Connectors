{
  "name": "@connectors/cli",
  "version": "0.1.0",
  "description": "Interactive CLI for Connectors platform",
  "main": "dist/index.js",
  "bin": {
    "connectors": "./bin/connectors"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "connectors",
    "mcp",
    "cli",
    "ai-tools",
    "interactive",
    "semantic-routing"
  ],
  "author": "Connectors Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raghavpatnecha/Connectors.git",
    "directory": "gateway/packages/cli"
  },
  "dependencies": {
    "@connectors/sdk": "workspace:*",
    "commander": "^12.1.0",
    "inquirer": "^9.2.19",
    "ora": "^8.0.1",
    "chalk": "^5.3.0",
    "conf": "^12.0.0",
    "table": "^6.8.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
