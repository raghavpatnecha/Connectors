{
  "name": "@connectors/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for the Connectors Platform - AI Agent Integration with 99% token reduction",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "keywords": [
    "connectors",
    "mcp",
    "ai-agents",
    "sdk",
    "typescript",
    "model-context-protocol",
    "integration",
    "semantic-routing"
  ],
  "author": "Connectors Platform",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raghavpatnecha/Connectors.git",
    "directory": "gateway/packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/raghavpatnecha/Connectors/issues"
  },
  "homepage": "https://github.com/raghavpatnecha/Connectors#readme",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
